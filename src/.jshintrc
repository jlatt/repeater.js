// -*- mode: js -*-
{
    "browser": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "globals": {"_": false},
    "immed": true,
    "indent": 4,
    "jquery": true,
    "latedef": true,
    "maxdepth": 3,
    "maxlen": 80,
    "maxparams": 5,
    "maxstatements": 25,
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonew": true,
    "plusplus": true,
    "quotmark": "single",
    "regexp": false,
    "trailing": true,
    "undef": true,
    "unused": true
}
